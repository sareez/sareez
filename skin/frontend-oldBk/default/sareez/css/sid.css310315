.sareez-header .sareez-nav ul>li>ul{position: absolute; margin-top: 9px; z-index: 2;}
.sareez-header .nav-new-arrivals-ul{display: none; padding-left: 0px; margin-left: -15px;}
.sareez-header .nav-premium-ul{display: none; padding-left: 0px; margin-left: -25px;}
#nav-new-arrivals-ul{margin-left: -30px;}
#nav-premium-ul{ margin-left: -40px;}
.sareez-header .sareez-nav ul>li>ul>li {list-style: none; float: none;border-left:1px solid #dbdbdb; border-right:1px solid #dbdbdb; border-top: 1px solid #dbdbdb; border-bottom: 1px solid #dbdbdb;background: #ffffff;}
.sareez-header .sareez-nav ul>li>ul>li:hover{ background: #cb1732; }
.sareez-header .sareez-nav ul>li>ul>li a:hover{border-bottom: none; background: #cb1732; color: #ffffff;}

.sareez-header .sareez-nav ul>li>ul>li a{display: block; text-decoration: none; font-size:12px; font-weight:bold; color: #333333;padding: 0px;  margin-right: 0px; border-bottom: none; width: 120px; padding: 8px 10px; text-align: center;}
