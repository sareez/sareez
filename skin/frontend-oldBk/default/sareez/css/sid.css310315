/*  Product Combo CSS */
        .product-combo{border: 1px solid #e9eaea; width: 163px; font-family: arial, helvetica, sans-serif; overflow: hidden}
        .product-combo-heading{color: #0f97b1; margin: -6px -5px 0px -6px; padding: 10px; text-align: left; font-weight: bold; font-size: 10px; margin-bottom:15px;}


        .product-combo-thumbnail img{float:left; margin-right: 5px;}
        .product-combo-description{font-size: 11px; color: #494949; margin-bottom: 5px;}
        .product-combo-price{font-size: 14px; font-weight: bold; color: #9a0000; padding-left:5px;}
        .product-combo .fa-lg{color: #343434; margin-left: 25px; margin-bottom:10px;}
        .product-combo-clear{clear: both; padding-top: 15px;}
		.product-combo .product-combo-thumbnail-checkbox{float:left; margin-top:1px;}
/*     Product Combo END */ 


/* Product Slider */ 
#product-combo-slider{width: 396px; height: 100px; overflow: hidden;}
